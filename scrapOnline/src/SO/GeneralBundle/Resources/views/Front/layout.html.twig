        <script>
            function reportData(object) {
                //console.log(data);
                if (object.url != "unknown") {
                    //console.log(object);
                    $.get(object.url, function(data) {
                        $(""+object.replaced+"").replaceWith(data);
                    });
                }
            }
            function getParameterByName(name, url) {
                var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                        results = regex.exec(url);
                return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
            }
            window.addEventListener('popstate', function(event) {
                //console.log('popstate');
                reportData(event.state || {url: "unknown", name: "undefined", location: "undefined"});
            });
            $(document).ready(function() {
                $('#featured-control span a').click(function(e) {
                    var slider = $("#main-slider-wrapper");
                    if (slider.css('display') == 'block') {
                        //$('#main-slider').liquidSlider({autoSlide: true});
                        $('#featured-control span').css('font-size', 10);
                        $('#featured-control span').css('margin-bottom', 4);
                        $('#featured-control span a').attr('name', 'start');
                        $('#featured-control span a').html(' v v v ');
                    } else {
                        // $('#main-slider').liquidSlider({autoSlide: false});
                        $('#featured-control span').css('font-size', 13);
                        $('#featured-control span').css('margin-bottom', 0);
                        $('#featured-control span a').attr('name', 'stop');
                        $('#featured-control span a').html(' ^ ^ ^ ');
                    }
                    slider.slideToggle('slow');
                });
            });
        </script>
