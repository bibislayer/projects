<?php

namespace VM\MakerBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Advert
 */
class Advert
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $address;

    /**
     * @var string
     */
    private $locality;

    /**
     * @var string
     */
    private $country;

    /**
     * @var string
     */
    private $lng;

    /**
     * @var string
     */
    private $lat;

    /**
     * @var string
     */
    private $text_introduction;

    /**
     * @var string
     */
    private $text_description;

    /**
     * @var integer
     */
    private $approbation;

    /**
     * @var integer
     */
    private $published;

    /**
     * @var \DateTime
     */
    private $published_at;

    /**
     * @var \DateTime
     */
    private $start_date;

    /**
     * @var \DateTime
     */
    private $end_date;

    /**
     * @var boolean
     */
    private $closed;

    /**
     * @var string
     */
    private $period;

    /**
     * @var string
     */
    private $type;

    /**
     * @var integer
     */
    private $advert_type;

    /**
     * @var string
     */
