{% extends "PokerGeneralBundle::Middle/layout.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block fos_user_content %}
<div class="content _70">
    <div class="grid _100 left">
        <div class="block_border">
            <div class="block_design">
                <div class=" block_content text_center">
                    <p class="succes"><span>Bienvenue {{user.UserProfile.firstName }}</span></p>
                    <p class="succes">Votre inscription au site Scoring-Line et la création de votre entreprise {{ enterprise.name }} ont bien été pris en compte .</p>
                    <div class="box">
                        <div class="mail"></div>
                            <p>Un mail de confirmation a été envoyé à l'adresse que vous avez indiquée.</p>
                            <p>Pensez à verifier vos spam.</p>
                        <div class="stop"></div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
{% endblock fos_user_content %}

